{
  "version": 2,
  "builds": [
    {
      "src": "/deploy-test",
      "use": "@vercel/node",
      "config": {
        "functions": {
          "prebuild": "prisma generate"
        }
      }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/deploy-test/$1" }
  ]
}

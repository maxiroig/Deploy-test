{
  "version": 2,
  "builds": [
    {
      "src": "nuxt.config.js",
      "use": "@nuxtjs/vercel-builder",
      "config": {
        "dev": false
      }
    }
  ],
  "functions": { "prebuild": "npx prisma generate" },
  "install": { "include": ["prisma"], "additionalInstallSteps": ["pnpm install --no-frozen-lockfile"] },
  "env": {
    "YOUR_ENV_VARIABLE": "value"
  }
}
